<h1>Sign In</h1><%= view_helpers.link_to('Create a new Tenant account', '#', {id: 'create_account_link', 'data-type':'tenant'}) %>
<%= view_helpers.link_to('Sign in as Landlord', '#', {id: 'toggle_sign_in_form', 'data-id':'landlord'}) %>
<%= view_helpers.form_tag('#',{method: 'POST', id: 'sign_in_form'}) %>
	<label>Email</label>
	<%= view_helpers.text_field_tag('email') %>
	<label>Password</label>
	<%= view_helpers.text_field_tag('password') %>	
	<%= view_helpers.hidden_field_tag('identity', 'Tenant') %>		
	<%= view_helpers.submit_tag('Sign In', {id: 'sign_in_submit'})%>		
<% view_helpers.end_form_tag %>